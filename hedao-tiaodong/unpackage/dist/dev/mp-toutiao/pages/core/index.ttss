.contain {
	width: 100%;
}
._figure {
    position: relative;
    margin-top: -74rpx;
    width: 100%;
    height: auto;
    padding: 0 36rpx;
    box-sizing: border-box;
    margin-bottom: 48rpx;
    z-index: 2;
}
._figure .photoBox {
    display: flex;
    position: relative;
    align-items: center;
    margin-right: 20rpx;
    border: none;
    padding: 0;
    margin-bottom: 0;
}
.img-box-border {
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
    width: 200rpx;
    height: 200rpx;
    border-radius: 50%;
	margin-right: 0;
    margin-bottom: 42rpx;
    background: url(data:image/png;base64,) no-repeat center;
    background-size: cover;
    padding: 10rpx;
    box-sizing: border-box;
}
._figure .photoBox .img-box-border>._img {
    width: 100%;
    height: 100%;
    border-radius: 50%;
}
._figure .user-age-box {
    align-items: center;
    justify-content: center;
    position: relative;
    display: flex;
    flex-direction: column;
}
._figure .user-age-box .user {
    font-size: 38rpx;
    font-weight: bold;
    margin-top: 36rpx;
}
.userFlag {
    display: flex;
    align-items: center;
    font-size: 28rpx;
    color: #999;
    margin: 16rpx 0 4rpx 0;
}
.userFlag ._img {
    width: 27rpx;
    height: 27rpx;
    margin-right: 14rpx;
}
._figure .user-age-box .new-user {
    display: flex;
}
._figure .user-age-box .user-age, ._figure .user-age-box .link-wb, #btn_wb {
    width: 80rpx;
    height: 36rpx;
    font-size: 20rpx;
    border-radius: 3rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    border: 1rpx solid #F0F0F0;
    color: #999999;
    margin-right: 10rpx;
    margin-top: 14rpx;
}
._figure .user-info {
    padding-bottom: 48rpx;
    padding: 26rpx 0;
    text-align: center;
}
.user-info>._div {
    word-break: break-word;
    font-size: 28rpx;
    line-height: 1.5;
    color: #999999;
}
.member-link {
    display: flex;
    justify-content: space-between;
    align-items: center;
    position: relative;
    width: 726rpx;
    height: 80rpx;
    margin: 0 auto;
    margin-bottom: 48rpx;
}
.member-link > ._div {
    width: 100%;
    height: 78rpx;
    color: #666666;
    font-size: 28rpx;
    border-radius: 10rpx;
    border: 1rpx solid #f0f0f0;
    display: flex;
    align-items: center;
    justify-content: center;
    cursor: pointer;
}
.member-link > ._div {
    width: 100%;
    height: 78rpx;
    color: #666666;
    font-size: 28rpx;
    border-radius: 10rpx;
    border: 1rpx solid #f0f0f0;
    display: flex;
    align-items: center;
    justify-content: center;
    cursor: pointer;
}
.contentUserWrap {
    height: 92rpx;
    border-top: 1rpx solid #f0f0f0;
    border-bottom: 10rpx solid #f0f0f0;
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 24rpx;
    padding: 0 24rpx;
    box-sizing: border-box;
    margin-bottom: 20rpx;
}
.leftImg {
    font-weight: normal;
    display: flex;
    align-items: center;
}
.leftImg ._img {
    height: 36rpx;
    width: 36rpx;
    border-radius: 50%;
    margin-right: -14rpx;
    border: 1rpx solid white;
}
.leftImg ._span {
    color: #999;
    margin-left: 24rpx;
}
.contentShareWrap {
    display: flex;
    align-items: center;
    color: #333;
}
.contentShareWrap ._img {
    width: 14rpx;
    height: 24rpx;
    margin-left: 20rpx;
}
.menu {
    width: 100%;
    height: 60rpx;
    color: #999;
    font-size: 32rpx;
    display: flex;
    justify-content: center;
}
.menu>._div {
    width: 22%;
    display: inline-block;
    cursor: pointer;
    text-align: center;
    height: 60rpx;
}
.menu .menu-li {
    width: 100% !important;
    width: 197rpx;
    height: 60rpx;
    position: relative;
}
.menu .blackTxt {
    color: #333;
}
.menu .redLine {
    width: 44rpx;
    height: 6rpx;
    background-color: #37a2f2;
    border-radius: 3rpx;
    position: absolute;
    z-index: 9;
    bottom: 6rpx;
    left: 50%;
    margin-left: -22rpx;
}
.core-index-container {
    width: 100%;
    background-color: #f0f0f0;
    padding: 8rpx 0;
}
.core-index-box {
    padding: 48rpx 36rpx;
    box-sizing: border-box;
    background-color: #fff;
    font-size: 28rpx;
    margin-bottom: 8rpx;
}
.core-index-title {
    font-size: 38rpx;
    font-weight: bold;
    margin-bottom: 24rpx;
}
/* 会员模块 */
.member-card {
    padding: 12rpx 0 0 0;
    box-sizing: border-box;
    color: #c89c5b;
    position: relative;
    background-color: white;
}
.member-card-new {
    position: relative;
    height: 420rpx;
    border-radius: 20rpx;
    color: white;
    overflow: hidden;
}
.member-card-new .opacity {
    position: absolute;
    top: 0;
    left: 0;
    background: rgba(0,0,0,.3);
    width: 100%;
    height: 100%;
}
.member-card-new .box {
    padding: 36rpx;
    box-sizing: border-box;
    width: 100%;
    height: auto;
    display: flex;
    align-items: flex-start;
    justify-content: space-between;
    font-size: 24rpx;
    z-index: 3;
}
.member-card-new .member-user-img {
    width: 101rpx;
    height: 101rpx;
    border-radius: 50%;
    border: 4rpx solid rgba(255,255,255,0.7);
    margin-right: 24rpx;
    z-index: 3;
}
.member-card-new .size-wrap {
    flex: 1;
    height: 108rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    z-index: 2;
    color: #c89c5b;
}
.member-card-new .size-title {
    font-size: 32rpx;
    line-height: 32rpx;
    z-index: 3;
    padding-bottom: 20rpx;
    color: white;
}
.member-card-new .size-txt {
    font-size: 24rpx;
    line-height: 24rpx;
    z-index: 3;
    color: rgba(255, 255, 255, .7);
}
.member-card-new .member-vip-img {
    width: 90rpx;
    height: 40rpx;
    margin-top: 14rpx;
    z-index: 3;
}
.member-card .flex-betweem, .member-card-new .flex-betweem {
    display: flex;
    align-items: center;
    justify-content: space-between;
    height: 88rpx;
    width: 100%;
    padding: 0 36rpx;
    box-sizing: border-box;
    font-size: 28rpx;
    color: #987238;
    z-index: 3;
}
.member-card-new .flex-betweem, .white-num, .member-card-new .flex-betweem ._span {
    color: white;
    z-index: 3;
}
.member-line {
    width: 100%;
    height: 1rpx;
    background-color: rgba(255,255,255,0.3);
    z-index: 3;
    position: absolute;
    top: 182rpx;
    left: 0;
}
.white-num ._span {
    color: white;
    font-weight: bold;
    padding-left: 20rpx;
}
.white-num ._span:first-child {
    padding-right: 20rpx;
}
.member-card-new .btn-white {
    height: 50rpx;
    line-height: 20rpx;
    text-align: center;
    box-sizing: border-box;
    border-radius: 25rpx;
    background-color: #ffffff;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 24rpx;
    color: #333333;
    width: 200rpx;
    position: absolute;
    bottom: 40rpx;
    left: 36rpx;
    z-index: 3;
}
.support-wrap {
    border-bottom: none;
    border-top: 1px solid #f0f0f0;
    padding: 0;
    width: 100%;
    box-sizing: border-box;
    position: relative;
    background-color: white;
}
.support-wrap ._h3 {
	font-size: 22rpx;line-height: 22rpx;color: #999999;padding: 36rpx 0 16rpx 0;
}
.support-wrap > ._div{
	font-size: 26rpx;
}
.core-index-blue-btn, .core-index-txtBlue-btn, .core-index-gray-btn {
    height: 88rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #fff;
    background-color: #37a2f2;
    font-size: 32rpx;
    border-radius: 10rpx;
    cursor: pointer;
    margin-top: 36rpx;
}
/* 目标 */
.core-index-target {
    margin-bottom: 38rpx;
    position: relative;
}
.core-index-txt-block {
    display: flex;
    align-items: center;
    color: #333;
}
.core-index-txt-block > ._span {
    display: inline-block;
    font-size: 28rpx;
    margin-right: 36rpx;
}
.txt-block-blue {
    color: #37a2f2;
}
.font-bold {
    font-weight: bold;
}
.core-index-right-absolute {
    position: absolute;
    right: 0;
}
.core-index-line10 {
    width: 100%;
    height: 8rpx;
    background-color: #dedede;
    border-radius: 4rpx;
    margin: 22rpx 0;
    position: relative;
}
.line10-blue {
    background-color: #37a2f2;
    position: absolute;
    left: 0;
    height: 8rpx;
    max-width: 100%;
    border-radius: 4rpx;
}
.core-index-txt-font24 {
    font-size: 24rpx;
}
.list {
    display: flex;
}
.list ._ul {
    flex-direction: column;
    justify-content: flex-start;
    position: relative;
    width: 100%;
    display: flex;
    flex-wrap: wrap;
    padding: 10rpx;
    padding-bottom: 0;
    box-sizing: border-box;
}
.list ._ul ._li:nth-child(odd) {
    margin-right: 1.5%;
}
.list ._ul ._li {
    width: 100%;
    margin-bottom: 10rpx;
    background-color: white;
}
.list ._ul ._li>._a {
    position: relative;
    display: block;
    cursor: pointer;
    height: 508rpx;
    overflow: hidden;
}
.list ._ul ._li>._a>._img {
    width: 100%;
    vertical-align: top;
}
.list ._ul ._li >._div {
    position: relative;
    width: 100%;
    height: auto;
    font-size: 24rpx;
    line-height: 24rpx;
    color: #333;
    display: flex;
    justify-content: space-between;
    padding: 14rpx;
    box-sizing: border-box;
}
.list ._ul ._li .l-txt {
    position: relative;
    height: 60rpx;
    width: 240rpx;
}
.list ._ul ._li .l-txt ._h1 {
    margin: 2rpx 0 10rpx 0;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}
.list ._ul ._li .l-txt>._span {
    display: block;
    font-size: 24rpx;
    line-height: 24rpx;
    color: #999;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}
.left-right-padding36 {
    width: 100%;
    padding: 0 36rpx;
    box-sizing: border-box;
}
.core-index-gray-btn {
    background-color: #f5fafe;
    color: #37a2f2;
}
.top-icon-btn {
	height: 88rpx;
}

