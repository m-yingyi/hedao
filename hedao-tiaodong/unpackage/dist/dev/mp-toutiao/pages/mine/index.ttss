page {
	background-color: #f0f0f0;
}
.contain {
	width: 100%;
}
.auth {
    position: relative;
    width: 100%;
    display: flex;
    align-items: flex-end;
    font-size: 28rpx;
    background-color: white;
    padding: 36rpx;
    box-sizing: border-box;
}
.userFeed {
    border-top: 1rpx solid #f0f0f0;
    border-bottom: none;
}
.auth .userAuth {
    position: relative;
    display: flex;
    align-items: center;
    justify-content: center;
}
.auth .msgAuth {
    display: flex;
    flex-direction: column;
    margin-left: 36rpx;
}
.auth-msg, .auth .setAuth{
    position: relative;
    border-radius: 33rpx;
    border: 1rpx solid #f0f0f0;
    display: flex;
    align-items: center;
    justify-content: center;
    height: 64rpx;
    color: #333333;
    width: 72%;
    margin-left: 32rpx;
}
.setAuth ._img {
    width: 38rpx;
    height: 38rpx;
    vertical-align: middle;
}
.auth .msgAuth ._a {
    color: #333;
}
.msgAuth .auth-msg {
    width: 358rpx;
    font-size: 28rpx;
    margin-top: 30rpx;
	margin-left: 0;
}
.userFeed .userAuth .head-photo {
    background-image: none;
}
.auth .userAuth .head-photo {
	border-radius: 50%;
	border: 1rpx solid #f0f0f0;
    width: 130rpx;
    height: 130rpx;
    z-index: 3;
}
.auth .userAuth >._img {
    position: absolute;
    border-radius: 50%;
    width: 130rpx;
    height: 130rpx;
    vertical-align: middle;
}
.goMyBought {
	padding: 36rpx 98rpx;
    background: white;
    position: relative;
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-top: 88rpx;
}
.goMyBought .mybought-box ._a {
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    font-size: 36rpx;
    font-weight: bold;
    color: #333333;
}
.link .base-space{
    margin-top: 88rpx;
}
.sect-txt {
    font-size: 24rpx;
    color: #999;
    padding-top: 6rpx;
    font-weight: normal;
}
.no-login {
    align-items: center;
}
.click-login {
    display: flex;
    align-items: center;
    color: #37a2f2;
    margin-left: 36rpx;
    font-size: 32rpx;
    font-weight: 600;
}

