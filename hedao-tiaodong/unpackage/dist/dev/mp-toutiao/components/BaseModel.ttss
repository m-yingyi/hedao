












































/* 基础弹窗 */
.base-model.data-v-0f7c480d {
    top: 0;
    left: 0;
    position: fixed;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, .7);
    z-index: 99;
}
.base-model .base-model-box.data-v-0f7c480d {
    position: absolute;
    width: 480rpx;
    background-color: white;
    text-align: center;
    border-radius: 20rpx;
    left: 50%;
    top: 480rpx;
    transform: translateX(-50%);
    -webkit-transform: translateX(-50%);
    min-height: 217rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    color: #333;
    padding-top: 50rpx;
}
.base-model-title.data-v-0f7c480d {
    font-size: 32rpx;
    font-weight: bold;
}
.base-model-desc.data-v-0f7c480d {
    font-size: 24rpx;
    color: #999;
    padding: 9rpx 0 52rpx 0;
}
.base-model-footer.data-v-0f7c480d {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: space-between;
    height: 90rpx;
    font-size: 32rpx;
}
.base-model-line.data-v-0f7c480d {
    height: 1rpx;
    width: 100%;
    background: #f0f0f0;
}
.base-model-footer_submit.data-v-0f7c480d,
.base-model-footer_cancle.data-v-0f7c480d {
    color: #ff4343;
    text-align: center;
    cursor: pointer;
    line-height: 90rpx;
    width: 50%;
}
.base-model-footer_cancle.data-v-0f7c480d {
    color: #999;
    border-left: 1rpx solid #f0f0f0;
}

